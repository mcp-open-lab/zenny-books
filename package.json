    "test": "vitest",
    "test:integration": "vitest --run --include '**/*.integration.ts'",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
