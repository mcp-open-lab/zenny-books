  "private": true,
  "scripts": {
    "dev": "next dev",
    "inngest": "npx inngest-cli@latest dev --sdk-url http://localhost:3000/api/inngest",
    "dev:inngest": "npx inngest-cli@latest dev --sdk-url http://localhost:3000/api/inngest",
    "dev:vercel": "vercel dev",
